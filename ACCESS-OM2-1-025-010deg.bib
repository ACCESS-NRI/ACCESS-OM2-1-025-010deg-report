	Keywords = {Tracking East Antarctic Sea Ice Satellite Envisat},
	Note = {ISSN 0034-4257},
	Number = {9},
	Pages = {2267-2276},
	Title = {Semi-automated feature-tracking of {East Antarctic} sea ice from {Envisat ASAR} imagery},
