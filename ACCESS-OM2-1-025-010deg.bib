	Doi = {10.1029/2000JC000577},
	Eprint = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2000JC000577},
	Journal = {Journal of Geophysical Research: Oceans},
	Number = {C9},
	Pages = {19527-19546},
	Title = {Velocity structure and transport of the {Indonesian Throughflow} in the major straits restricting flow into the {Indian Ocean}},
	Url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2000JC000577},
	Volume = {106},
	Year = {2001},
