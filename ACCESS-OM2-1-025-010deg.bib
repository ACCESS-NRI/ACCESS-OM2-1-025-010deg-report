	Keywords-Plus = {SOUTHERN-OCEAN; SIMULATION; CYCLE; VARIABILITY; CIRCULATION; DESIGN; SYSTEM; CCSM4},
	Number = {1},
	Orcid-Numbers = {Marsland, Simon/0000-0002-5664-5276 Uotila, Petteri/0000-0002-2939-7561},
	Pages = {121-143},
