	Journal = {Journal of Geophysical Research: Oceans},
	Keywords = {mixed-layer, climate model, ventilation, Southern Ocean},
	Number = {4},
	Pages = {1845-1862},
	Title = {Assessment of {Southern Ocean} mixed-layer depths in {CMIP5} models: Historical bias and forcing response},
	Url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1002/jgrc.20157},
